<h2>Details</h2>
<section class="section">
    <div class="card">
        <div class="card-body">
            @*<h6 class="text-primary mb-3"> Personal Details</h6>*@
            <div class="row">

                <div class="col-md-4">
                    <div class="form-group">
                        @Html.LabelFor(model => model.TechProblemType, htmlAttributes: new { @class = "required-field" })
                        @Html.DropDownList("TechProblemType", new SelectList(ViewBag.ProblemList, "ProblemTypeId", "Name"), "Select Problem", new { id = "TechProblemType", @class = "form-control", @readonly = "readonly" })
                        @Html.ValidationMessageFor(model => model.TechProblemType, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="form-group">
                        @Html.LabelFor(model => model.Description, htmlAttributes: new { @class = "required-field" })
                        @Html.EditorFor(model => model.Description, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        @Html.ValidationMessageFor(model => model.Description, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        @Html.LabelFor(model => model.PhoneNumber, htmlAttributes: new { @class = "required-field" })
                        @Html.EditorFor(model => model.PhoneNumber, new { htmlAttributes = new { @class = "form-control", @maxlength = "10", @readonly = "readonly" } })
                        @Html.ValidationMessageFor(model => model.PhoneNumber, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="form-group">
                        @Html.LabelFor(model => model.AnyDeskDetail, htmlAttributes: new { @class = "required-field" })
                        @Html.EditorFor(model => model.AnyDeskDetail, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        @Html.ValidationMessageFor(model => model.AnyDeskDetail, "", new { @class = "text-danger" })
                    </div>
                </div>
                @*<div class="col-md-4">
                    <div class="form-group">
                        @Html.LabelFor(model => model.Status, htmlAttributes: new { @class = "required-field" })
                        @Html.DropDownList("Status", (SelectListItem[])ViewBag.EditStatus, htmlAttributes: new { @class = "form-control", @readonly = "readonly" })
                        @Html.ValidationMessageFor(model => model.Status, "", new { @class = "text-danger" })
                    </div>
                </div>*@
                @*<div class="row">*@
                <div class="col-md-4">
                    <div class="form-group">
                        @Html.LabelFor(model => model.Screenshot, htmlAttributes: new { @class = "required-field" })
                       
                        @Html.EditorFor(model => model.Screenpic, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })


                    </div>
                </div>

                @*</div>*@

            </div>

          
            <div class="text-left">
                @Html.ActionLink("Cancel", "Index")
